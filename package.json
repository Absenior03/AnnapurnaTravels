{
  "name": "annapurna-tours-and-travels",
  "version": "0.1.0",
  "description": "A tourism website for Annapurna Tours and Travels",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "chmod +x fix-imports.sh && ./fix-imports.sh && NEXT_TELEMETRY_DISABLED=1 NEXT_DISABLE_ETW=1 NODE_PATH=. DISABLE_ESLINT_PLUGIN=true NEXT_DISABLE_ESLINT=1 NODE_OPTIONS=--max_old_space_size=4096 SWC_MINIFY=true next build --no-lint",
    "postbuild": "next-image-export-optimizer",
    "start": "next start",
    "lint": "next lint",
    "prepare-deploy": "chmod +x fix-imports.sh && ./fix-imports.sh"
  },
  "_moduleAliases": {
    "@": "./src",
    "@/components": "./components",
    "@/context": "./context"
  },
  "dependencies": {
    "firebase": "^9.23.0",
    "framer-motion": "^10.16.4",
    "module-alias": "^2.2.3",
    "next": "14.0.3",
    "next-superjson-plugin": "^0.5.10",
    "pexels": "^1.4.0",
    "razorpay": "^2.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-config-next": "14.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5"
  }
}
