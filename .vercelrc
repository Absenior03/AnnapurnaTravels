{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DISABLE_ESLINT_PLUGIN": "true",
    "NEXT_DISABLE_ESLINT": "1",
    "NODE_PATH": ".",
    "SWC_MINIFY": "true",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_DISABLE_ETW": "1"
  }
} 